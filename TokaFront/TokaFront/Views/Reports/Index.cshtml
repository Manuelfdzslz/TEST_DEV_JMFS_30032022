@{
    Layout = "_Layout";
    ViewData["Title"] = "Reportes";
}

<div class="container">
    <div class=" row">
        <div class="col-md-12  text-right p-3">
            <a type="button" class="btn btn-primary" href="/Reports/CreateReport" > Generar Reporte</a>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <table class="table table-flush table-no-bordered table-hover display responsive nowrap" cellspacing="0" width="100%" style="width:100%" id="datatable">
                <thead class="thead-light">
                    <tr>
                        <th>IdCliente</th>
                        <th>Nombre</th>
                        <th>Paterno</th>
                        <th>Materno</th>
                        <th>IdViaje</th>
                        <th>RFC</th>
                        <th>Sucursal</th>
                        <th>Razon Social</th>
                        <th>Fecha Registro Empresa</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>

    </div>
</div>

@section Scripts {
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/js/Reports.js" asp-append-version="true"></script>
}
